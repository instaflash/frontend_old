{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "indent": [true, "tabs", 4],
    "quotemark": [true, "single"],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-var-requires": false,
    "no-console": false,
    "interface-name": [true, "never-prefix"],
    "trailing-comma": [true, {"multiline": "never", "singleline": "never"}]
  },
  "rulesDirectory": []
}
